  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8bb4d8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#00ba7c"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#4F5B6F"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#4F5B6F"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#f7f9f9"
      }
    },
    {
      "name": "Variable Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#8bb4d8"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1e80c7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.logical",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#5FF4AE"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#93abd0"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#bdbec1"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#a0b0c0"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#f7f9f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#1e80c7"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#5FF4AE"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#1e80c7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Module",
      "scope": [
        "entity.name.type.module",
        "support.module"
      ],
      "settings": {
        "foreground": "#8bb4d8"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#4F5B6F"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#8bb4d8"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E11D1DFF"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#bdbec1",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "CSS ID",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#bdbec1"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#00ba7c"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#1e80c7"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#1e80c7"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#1e80c7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#f7f9f9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#f7f9f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#1e80c7"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#00ba7c"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#8bb4d8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#E11D1DFF"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#00ba7c"
      }
    },
    {
      "name": "Markup Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#bdbec1"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#4F5B6F"
      }
    },
    {
      "name": "SQL",
      "scope": [
        "keyword.other.DML.sql"
      ],
      "settings": {
        "foreground": "#1e80c7"
      }
    },
    {
      "name": "Python Self",
      "scope": [
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#4F5B6F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Decorator",
      "scope": [
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#bdbec1"
      }
    },
    {
      "name": "JavaScript This",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#4F5B6F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "TypeScript Type",
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.tsx"
      ],
      "settings": {
        "foreground": "#bdbec1"
      }
    },
    {
      "name": "React Component",
      "scope": [
        "support.class.component.tsx",
        "support.class.component.jsx"
      ],
      "settings": {
        "foreground": "#a0b0c0"
      }
    },
    {
      "name": "All Punctuation Override",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition",
        "punctuation.section",
        "meta.brace",
        "meta.brace.round",
        "meta.brace.square", 
        "meta.brace.curly",
        "punctuation.definition.typeparameters.begin.ts",
        "punctuation.definition.typeparameters.end.ts",
        "punctuation.definition.parameters.begin.ts",
        "punctuation.definition.parameters.end.ts",
        "punctuation.definition.block.ts",
        "meta.brace.round.ts",
        "meta.brace.square.ts",
        "meta.brace.curly.ts"
